<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/san_common_dimens_8dp" />
            <solid android:color="@color/san_color_00ca89_90" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <corners android:radius="@dimen/san_common_dimens_8dp" />
            <solid android:color="@color/san_black_transparent_30" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/san_common_dimens_8dp" />
            <solid android:color="@color/san_color_00ca89_90" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/san_common_dimens_8dp" />
            <solid android:color="@color/san_color_e9e9e9" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/san_common_dimens_8dp" />
            <solid android:color="@color/san_color_00ca89" />
        </shape>
    </item>
</selector>