<?xml version="1.0" encoding="utf-8"?>
<selector android:exitFadeDuration="@android:integer/config_mediumAnimTime"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/san_color_00ca89" />
            <corners android:radius="@dimen/san_common_dimens_6dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/san_color_00ca89_90" />
            <corners android:radius="@dimen/san_common_dimens_6dp" />
        </shape>
    </item>
</selector>